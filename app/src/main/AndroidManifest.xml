<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.delivery_app_dath">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DELIVERY_APP_DATH">
        <activity
            android:name=".SplastActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".WalletActivity"
            android:exported="true"
            android:parentActivityName=".MainUserActivity" />
        <activity
            android:name=".SettingActivity"
            android:exported="true"
            android:parentActivityName=".MainUserActivity" />
        <activity
            android:name=".OrderActivity"
            android:exported="true"
            android:parentActivityName=".MainUserActivity" />
        <activity
            android:name=".MyDriverActivity"
            android:exported="true"
            android:parentActivityName=".MainUserActivity" />
        <activity
            android:name=".HelpCenterActivity"
            android:exported="true"
            android:parentActivityName=".MainUserActivity" />
        <activity
            android:name=".InformationActivity"
            android:exported="true"
            android:parentActivityName=".MainUserActivity" />
        <activity
            android:name=".MainUserActivity"
            android:exported="true"
            android:label="@string/title_activity_main_user"
            android:theme="@style/Theme.DELIVERY_APP_DATH.NoActionBar">
        </activity>
        <activity
            android:name=".RegisterActivity"
            android:exported="true" />
        <activity
            android:name=".LoginActivity"
            android:exported="true" />
    </application>

</manifest>